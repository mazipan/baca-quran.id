{
  "name": "quran-offline",
  "version": "1.0.0",
  "description": "Baca Al-Qur'an dimana saja, langsung dari web browser Anda, tidak perlu lagi install Apps. Tanpa iklan, tanpa analitik dan GRATIS sepenuhnya.",
  "author": "Irfan Maulana",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nuxt dev",
    "build": "cross-env NODE_ENV=production nuxt build",
    "minify:json": "minify-json dist/data/",
    "generate": "nuxt generate && yarn minify:json",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .eslintignore .",
    "lint:report": "eslint --ext .js,.ts,.vue --ignore-path .eslintignore . --fix -f html-extended -o eslint-report.html",
    "lint:fix": "eslint --ext .js,.ts,.vue --ignore-path .eslintignore . --fix",
    "lint:md": "remark readme.md -o --color",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/jest --coverage",
    "lh": "lighthousebot"
  },
  "dependencies": {
    "@babel/plugin-transform-typescript": "^7.4.0",
    "axios": "^0.18.0",
    "nuxt": "^2.6.1",
    "nuxt-i18n": "^5.8.5",
    "unfetch": "^4.1.0",
    "vue-ionicons": "^3.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@nuxt/typescript": "^2.6.1",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@nuxtjs/pwa": "^2.6.0",
    "@nuxtjs/sitemap": "^0.2.2",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.4",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "autoprefixer": "^9.5.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "24.7.1",
    "babel-plugin-dynamic-import-node-babel-7": "^2.0.7",
    "core-js": "^2.6.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-formatter-html-extended": "^1.0.2",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-nuxt": "^0.4.3",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "express": "^4.16.4",
    "fs-extra": "^7.0.1",
    "husky": "^2.0.0",
    "jest": "^24.7.1",
    "jest-serializer-vue": "^2.0.2",
    "less-loader": "^4.1.0",
    "lighthousebot": "https://github.com/GoogleChromeLabs/lighthousebot",
    "minify-json": "^1.0.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.11",
    "remark-cli": "^6.0.1",
    "remark-preset-lint-consistent": "^2.0.2",
    "remark-preset-lint-markdown-style-guide": "^2.1.2",
    "remark-preset-lint-recommended": "^3.0.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.3",
    "vue-class-component": "^7.0.2",
    "vue-i18n": "^8.10.0",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.7.0",
    "vue-property-decorator": "^8.1.0",
    "vuex-class": "^0.3.2"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run lint:md"
    }
  }
}
